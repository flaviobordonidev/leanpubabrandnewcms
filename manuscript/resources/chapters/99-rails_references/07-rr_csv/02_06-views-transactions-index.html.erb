<p id="notice"><%= notice %></p>

<h1>Transactions</h1>

<table>
  <thead>
    <tr>
      <th>Csvfilename</th>
      <th>Timestamp</th>
      <th>Id check</th>
      <th>Onlus</th>
      <th>Project</th>
      <th>Kind</th>
      <th>Verify</th>
      <th>Cents</th>
      <th>People name</th>
      <th>People email</th>
      <th>Extra</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transactions.each do |transaction| %>
      <tr>
        <td><%= transaction.csvfilename %></td>
        <td><%= transaction.timestamp %></td>
        <td><%= transaction.id_check %></td>
        <td><%= transaction.onlus %></td>
        <td><%= transaction.project %></td>
        <td><%= transaction.kind %></td>
        <td><%= transaction.verify %></td>
        <td><%= transaction.cents %></td>
        <td><%= transaction.people_name %></td>
        <td><%= transaction.people_email %></td>
        <td><%= transaction.extra %></td>
        <td><%= link_to 'Show', transaction %></td>
        <td><%= link_to 'Edit', edit_transaction_path(transaction) %></td>
        <td><%= link_to 'Destroy', transaction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Transaction', new_transaction_path %>
<%= link_to 'Import', import_path %>